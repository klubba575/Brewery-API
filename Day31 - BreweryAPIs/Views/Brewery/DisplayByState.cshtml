
@{
    ViewData["Title"] = "DisplayByState";
}

@model List<Brew>

<h1>DisplayByState</h1>

<table class="table">
    <tr>
        <th>name</th>
        <th>brewery_type</th>
        <th>state</th>

    </tr>

@for (int i = 0; i < @Model.Count; i++)
{
<tr>
    <td> @Model[i].name</td>
    <td> @Model[i].brewery_type</td>
    <td> @Model[i].state</td>
</tr>
  
    }


</table>
